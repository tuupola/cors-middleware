    "description": "PSR-7 CORS Middleware",
    "keywords": [
        "slim",
        "middleware",
        "cors"
    ],
    "homepage": "https://github.com/tuupola/cors-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Mika Tuupola",
            "email": "tuupola@appelsiini.net",
            "homepage": "http://www.appelsiini.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "neomerx/cors-psr7": "^1.0",
        "http-interop/http-server-middleware": "^1.0",
        "tuupola/http-factory": "^0.3.0"
